<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home"
    android:background="#ffffff">
    <LinearLayout
        android:layout_width="match_parent"
        android:id="@+id/l1"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:elevation="10sp"
        android:background="#fff"
        android:padding="5sp">
        <Button
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Prev Page"
            android:onClick="prev"
            android:layout_margin="10sp"
            android:background="@drawable/prev_gradient"
            android:textColor="#fff"/>
        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="50sp"
            android:layout_height="50sp"
            android:layout_weight="1"
            android:layout_gravity="center_vertical"
            android:transitionName="rgu"
            android:layout_margin="5sp"
            app:srcCompat="@drawable/ic_rgukt1" />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Next Page"
            android:onClick="next"
            android:layout_margin="10sp"
            android:background="@drawable/next_gradient"
            android:textColor="#fff"/>
    </LinearLayout>
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/l1"
        android:fillViewport="true"
        android:id="@+id/s1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/l1"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/n1"
                android:layout_margin="5sp">
                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:textSize="20sp"
                    android:onClick="notice1"
                    android:fontFamily="@font/title"
                    android:layout_marginLeft="10sp"
                    android:background="@drawable/bg"
                    android:padding="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/n1"
                android:layout_margin="5sp">
                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:textSize="20sp"
                    android:fontFamily="@font/title"
                    android:layout_marginLeft="10sp"
                    android:background="@drawable/bg"
                    android:padding="10sp"
                    android:onClick="notice2"/>
            </LinearLayout>
           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:orientation="vertical"
               android:background="@drawable/n1"
               android:layout_margin="5sp">
               <TextView
                   android:id="@+id/textView3"
                   android:fontFamily="@font/title"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:layout_weight="1"
                   android:text=""
                   android:textSize="20sp"
                   android:layout_marginLeft="10sp"
                   android:background="@drawable/bg"
                   android:padding="10sp"
                   android:onClick="notice3"/>
           </LinearLayout>
           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:orientation="vertical"
               android:background="@drawable/n1"
               android:layout_margin="5sp">
               <TextView
                   android:id="@+id/textView4"
                   android:fontFamily="@font/title"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:layout_weight="1"
                   android:text=""
                   android:textSize="20sp"
                   android:layout_marginLeft="10sp"
                   android:background="@drawable/bg"
                   android:padding="10sp"
                   android:onClick="notice4"/>
           </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/n1"
                android:layout_margin="5sp">
                <TextView
                    android:id="@+id/textView5"
                    android:fontFamily="@font/title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:textSize="20sp"
                    android:layout_marginLeft="10sp"
                    android:background="@drawable/bg"
                    android:padding="10sp"
                    android:onClick="notice5"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/n1"
                android:layout_margin="5sp">
                <TextView
                    android:id="@+id/textView6"
                    android:fontFamily="@font/title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:textSize="20sp"
                    android:layout_marginLeft="10sp"
                    android:background="@drawable/bg"
                    android:padding="10sp"
                    android:onClick="notice6"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/n1"
                android:layout_margin="5sp">
                <TextView
                    android:id="@+id/textView7"
                    android:fontFamily="@font/title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:textSize="20sp"
                    android:layout_marginLeft="10sp"
                    android:background="@drawable/bg"
                    android:padding="10sp"
                    android:onClick="notice7"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/n1"
                android:layout_margin="5sp">
                <TextView
                    android:id="@+id/textView8"
                    android:fontFamily="@font/title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:textSize="20sp"
                    android:layout_marginLeft="10sp"
                    android:background="@drawable/bg"
                    android:padding="10sp"
                    tools:ignore="Suspicious0dp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/n1"
                android:layout_margin="5sp">
                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:textSize="20sp"
                    android:fontFamily="@font/title"
                    android:layout_marginLeft="10sp"
                    android:background="@drawable/bg"
                    android:padding="10sp"
                    android:onClick="notice9"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/n1"
                android:layout_margin="5sp">
                <TextView
                    android:id="@+id/textView10"
                    android:fontFamily="@font/title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:textSize="20sp"
                    android:layout_marginLeft="10sp"
                    android:background="@drawable/bg"
                    android:padding="10sp"
                    android:onClick="notice10"/>
            </LinearLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</RelativeLayout>